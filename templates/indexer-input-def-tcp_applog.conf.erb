  tcp {
    port         => 9998
    type         => "applog"
    ssl_enable   => false
    #ssl_cacert   => "/etc/ssl/certs/ca.crt"
    #ssl_cert     => "/etc/ssl/certs/logstash.crt"
    #ssl_key      => "/etc/ssl/certs/logstash.key"
    tags         => [ "tcp" ]
    data_timeout => 600
  }

  tcp {
    port         => 9997
    type         => "nxlog"
    codec        => 'json'
    ssl_enable   => true
    ssl_cacert   => "/etc/ssl/certs/ca.crt"
    ssl_cert     => "/etc/ssl/certs/logstash.crt"
    ssl_key      => "/etc/ssl/certs/logstash.key"
    tags         => [ "windows" ]
    data_timeout => 600
  }

