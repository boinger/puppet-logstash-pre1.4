  tcp {
    format     => "json"
    charset    => "UTF-8"
    port       => <%= scope.lookupvar('logstash::config::tcp_port')%>
    type       => "eventlog"
    ssl_enable => 1
    ssl_cacert => /etc/ssl/certs/rapidssl_intermediate.pem
    ssl_cert   => /etc/ssl/certs/wildcard.icix.com.crt
    ssl_key    => /etc/ssl/certs/wildcard.icix.com.key
  }
